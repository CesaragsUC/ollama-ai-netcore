{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName" ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "Ollama": {
    "Endpoint": "http://localhost:11434/",
    "ChatModelPhi3": "phi3:3.8b",
    "ChatModelVision": "llama3.2-vision:11b-instruct-q4_K_M"

  },
  "ChatSettings": {
    "SystemMessage": "Hi there! Iâ€™m your AI assistant for PickleIQ...",
    "AssistantMessage": "Hi! I'm the PickleIQ Coach. How can I help?"
  }
}
